export OS_AUTH_URL=http://192.168.100.10:5000/v2/
export OS_REGION_NAME=RegionOne
export OS_TENANT_NAME=SNSApp
export OS_USERNAME=snsapp-infra-user
export OS_PASSWORD=passw0rd
export OS_DMZ_NET=`neutron net-list -f csv | awk -F, '$2 ~/dmz-net/ { print $1 }' | tr -d \"`
export OS_APP_NET=`neutron net-list -f csv | awk -F, '$2 ~/app-net/ { print $1 }' | tr -d \"`
export OS_DBS_NET=`neutron net-list -f csv | awk -F, '$2 ~/dbs-net/ { print $1 }' | tr -d \"`